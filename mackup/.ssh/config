Host *
  AddKeysToAgent yes
  UseKeychain yes

# Private GitHub instance
Host github
  User git
  HostName github.com
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_rsa

# tretton37/MEL Azure Devops instance
Host azure
  User git
  HostName ssh.dev.azure.com
  Preferredauthentications publickey
  IdentityFile ~/.ssh/mel_azure